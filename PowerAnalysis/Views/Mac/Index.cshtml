@model System.Collections.Generic.List<PowerAnalysis.Controllers.Chart>

@{
	ViewBag.Title = "MACs";
}

<h2>Marginal Abatement Cost Curves</h2>


<table>
	<thead>
		<th>Name</th>
		<th>Description</th>
	</thead>
	<tbody>
	@foreach (var item in Model) {
		<tr>
			<td>@Html.ActionLink(item.Name, "Set", new { id = item.Id.Substring(6) })</td>
			<td>@Html.DisplayFor(modelItem => item.Description)</td>
		</tr>
	}
	</tbody>
</table>



<p>
@Html.ActionLink("Load new mac data set", "Load")
</p>

